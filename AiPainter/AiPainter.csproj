<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows10.0.17763.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <StartupObject></StartupObject>
    <ApplicationIcon>Images\edit-image.ico</ApplicationIcon>
    <Platforms>AnyCPU;x64</Platforms>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OutputPath>bin\any\Debug\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <OutputPath>bin\any\Release\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x64'">
    <OutputPath>bin\x64\Debug\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x64'">
    <OutputPath>bin\x64\Release\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="dist\**" />
    <Compile Remove="release-x64\**" />
    <Compile Remove="_controls\**" />
    <EmbeddedResource Remove="dist\**" />
    <EmbeddedResource Remove="release-x64\**" />
    <EmbeddedResource Remove="_controls\**" />
    <None Remove="dist\**" />
    <None Remove="release-x64\**" />
    <None Remove="_controls\**" />
  </ItemGroup>  
  
  <ItemGroup>
    <None Remove=".editorconfig" />
    <None Remove=".gitignore" />
    <None Remove="AboutDialog.rtf" />
    <None Remove="AiPainter.csproj.DotSettings" />
    <None Remove="AiPainter.sln.DotSettings" />
    <None Remove="build-release-x64.cmd" />
    <None Remove="dist-build.cmd" />
    <None Remove="dist-pack.cmd" />
    <None Remove="dist-pack.exclude.txt" />
    <None Remove="pack-to-dist.cmd" />
    <None Remove="pack-to-dist.exclude.txt" />
    <None Remove="SyImageEditor.sln.DotSettings" />
    <None Remove="SyScreenshoter.sln.DotSettings" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Images\edit-image.ico" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Adapters\StableDiffusion\SdEmbeddingStuff\SdEmbeddingsContextMenu.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Adapters\StableDiffusion\SdLoraStuff\SdLorasContextMenu.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Adapters\StableDiffusion\SdModelsForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Adapters\StableDiffusion\SdEmbeddingStuff\SdEmbeddingForm.cs" />
    <Compile Update="Adapters\StableDiffusion\SdListItemDownloading.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="Adapters\StableDiffusion\SdLoraStuff\SdLorasForm.cs" />
    <Compile Update="Controls\WaitingDialog.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Controls\SmartImageList.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Adapters\StableDiffusion\SdListItemDownloading.resx">
      <SubType>Designer</SubType>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent" Condition="'$(BuildingInsideVisualStudio)' == 'true'">
    <Exec Command="if not exist $(OutDir)external mkdir $(OutDir)external&#xD;&#xA;if not exist $(OutDir)external\lama-cleaner mklink /D $(OutDir)external\lama-cleaner $(SolutionDir)..\_external\lama-cleaner\dist&#xD;&#xA;if not exist $(OutDir)external\ComfyUI mklink /D $(OutDir)external\ComfyUI $(SolutionDir)..\_external\ComfyUI&#xD;&#xA;if not exist $(OutDir)stable_diffusion_checkpoints mklink /D $(OutDir)stable_diffusion_checkpoints $(SolutionDir)..\_stable_diffusion_checkpoints&#xD;&#xA;if not exist $(OutDir)stable_diffusion_styles mklink /D $(OutDir)stable_diffusion_styles $(SolutionDir)..\_stable_diffusion_styles&#xD;&#xA;if not exist $(OutDir)stable_diffusion_vae mklink /D $(OutDir)stable_diffusion_vae $(SolutionDir)..\_stable_diffusion_vae&#xD;&#xA;if not exist $(OutDir)stable_diffusion_lora mklink /D $(OutDir)stable_diffusion_lora $(SolutionDir)..\_stable_diffusion_lora&#xD;&#xA;if not exist $(OutDir)stable_diffusion_embeddings mklink /D $(OutDir)stable_diffusion_embeddings $(SolutionDir)..\_stable_diffusion_embeddings&#xD;&#xA;&#xD;&#xA;" />
  </Target>

  <ItemGroup>
    <EditorConfigFiles Remove="C:\MyProg\ai-image-processing\AiPainter\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="AboutDialog.rtf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BigGustave" Version="1.0.6" />
    <PackageReference Include="SocketIOClient" Version="3.0.6" />
    <PackageReference Include="System.Management" Version="9.0.3" />
    <PackageReference Include="TagLibSharp" Version="2.3.0" />
  </ItemGroup>

  <ItemGroup>
    <EditorConfigFiles Remove="C:\MyProg\AiPainter\AiPainter\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <EditorConfigFiles Remove="C:\MyProg\AiPainter\AiPainter\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="ComfyWorkflowTemplates\" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ComfyWorkflowTemplates\img2img.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="ComfyWorkflowTemplates\interrogate.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="ComfyWorkflowTemplates\txt2img.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="ComfyWorkflowTemplates\txt2img_api.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="ComfyWorkflowTemplates\upscale.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>